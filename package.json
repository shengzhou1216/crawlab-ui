{
  "name": "crawlab-ui",
  "version": "0.6.0-17",
  "private": false,
  "author": {
    "name": "Marvin Zhang",
    "email": "tikazyq@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crawlab-team/crawlab-ui"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "serve": "vue-cli-service serve --port=8081",
    "serve:local": "vue-cli-service serve --port=8081 --mode local",
    "gen:index": "node ./scripts/gen-index.js",
    "gen:svg": "node ./scripts/gen-svg.js",
    "gen:dts": "node ./scripts/gen-dts.js",
    "gen:interfaces": "node ./scripts/gen-interfaces.js",
    "prebuild": "yarn run prebuild:gen",
    "prebuild:gen": "yarn run gen:index && yarn run gen:svg",
    "postbuild": "yarn run postbuild:gen",
    "postbuild:gen": "yarn run gen:dts && yarn run gen:interfaces",
    "postbuild:dist:local": "node ./scripts/copy-dist-local.js",
    "build": "yarn run prebuild && yarn run build:lib && yarn run postbuild",
    "build:dist:local": "yarn run build && yarn run postbuild:dist:local",
    "build:docker": "vue-cli-service build --mode docker",
    "build:local": "vue-cli-service build --mode local",
    "build:lib": "vue-cli-service build --target lib --mode production src/index.ts",
    "build:lib:watch": "vue-cli-service build --target lib --mode production --watch src/index.ts",
    "build:lib:local": "vue-cli-service build --target lib --mode local src/index.ts",
    "build:lib:dist": "yarn run build:lib && yarn run postbuild:dist:local",
    "build:lib:analyze": "vue-cli-service build --target lib --mode analyze src/index.ts",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "@fortawesome/fontawesome-common-types": "^6.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@popperjs/core": "^2.6.0",
    "@types/codemirror": "^0.0.103",
    "@types/echarts": "^4.9.8",
    "@types/getos": "^3.0.1",
    "@types/humanize-duration": "^3.25.0",
    "@types/javascript-time-ago": "^2.0.3",
    "@types/md5": "^2.2.1",
    "@types/pinyin": "^2.8.2",
    "@types/showdown": "^1.9.4",
    "@types/simplemde": "^1.11.8",
    "@types/url-join": "^4.0.1",
    "atom-material-icons": "^3.0.0",
    "axios": "^0.21.1",
    "codemirror": "^5",
    "core-js": "^3.6.5",
    "cron-parser": "^3.5.0",
    "cronstrue": "^1.114.0",
    "dayjs": "^1.10.5",
    "echarts": "^5.3.2",
    "element-plus": "^1.3.0-beta.10",
    "font-awesome": "^4.7.0",
    "getos": "^3.2.1",
    "humanize-duration": "^3.26.0",
    "javascript-time-ago": "^2.3.13",
    "md5": "^2.3.0",
    "normalize.css": "^8.0.1",
    "pinyin": "^2.10.2",
    "point-cluster": "^3.1.8",
    "showdown": "^1.9.1",
    "simplemde": "^1.11.2",
    "url-join": "^4.0.1",
    "vue": "^3.2",
    "vue-clipboard3": "^1.0.1",
    "vue-debounce": "^4.0.0",
    "vue-i18n": "9.1.9",
    "vue-router": "4.0.11",
    "vue3-dropzone": "^0.0.7",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-vuex": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "3.0.11",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/vue3-jest": "28",
    "babel-jest": "^28.1.1",
    "base64-img": "^1.0.4",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^6.0",
    "eslint": "^7",
    "eslint-plugin-vue": "^8.2.0",
    "esno": "^0.9.1",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.1",
    "jest": "^28.1.1",
    "klaw-sync": "^6.0.0",
    "node-sass": "6.0.0",
    "rd": "^2.0.1",
    "sass-loader": "^10.1.0",
    "scss-loader": "^0.0.1",
    "ts-jest": "^28.0.5",
    "ts-morph": "^12.0.0",
    "typescript": "^4.5.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    }
  },
  "main": "dist/crawlab-ui.umd.min.js",
  "files": [
    "dist",
    "public",
    "src",
    "typings",
    "babel.config.js",
    "vue.config.js"
  ],
  "typings": "typings/index.d.ts"
}
